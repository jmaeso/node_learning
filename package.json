{
  "name": "NodeLearning",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slipy12/node_learning.git"
  },
  "description": "App creation to learn electron and node basics",
  "main": "./app/main.js",
  "scripts": {
    "start": "ENVIRONMENT=DEV ./node_modules/.bin/electron .",
    "build": "electron-packager . --icon=images/sample_icon --overwrite",
    "webpack-build": "./node_modules/.bin/webpack --config webpack.config.js",
    "dev": "npm run watch & sleep 3 && npm run start",
    "kill-webpack": "pkill -f ./node_modules/.bin/webpack-dev-server",
    "watch": "npm run kill-webpack ; ./node_modules/.bin/webpack-dev-server --hot --inline"
  },
  "keywords": [
    "learning"
  ],
  "author": "Joan Maeso",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.1",
    "electron-packager": "^8.4.0",
    "electron-prebuilt": "^1.4.13",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "electron": "^1.4.14",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2"
  }
}
